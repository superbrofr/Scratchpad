#include "VigenereIndexer.h"
#include <iostream>

using namespace std;

int main()
{
	VigenereIndexer lScrambler("too many secrets", true);

	cout << lScrambler['a'];
	cout << lScrambler['c'];
	cout << lScrambler['t'];
	cout << lScrambler[' '];
	cout << lScrambler['i'];
	return 0;
}
