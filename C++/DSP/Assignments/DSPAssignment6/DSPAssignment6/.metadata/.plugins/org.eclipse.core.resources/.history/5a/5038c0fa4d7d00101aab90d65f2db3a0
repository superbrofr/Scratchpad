#ifndef DYNAMICQUEUE_H_
#define DYNAMICQUEUE_H_

#include "List.h"
#include <stdexcept>

template<class T>
class DynamicQueue
{
private:
	List<T> fElements;

public:
	bool isEmpty() const;
	int size() const;
	void enqueue(const T& aElement);
	const T& dequeue();
};

#endif /* DYNAMICQUEUE_H_ */
